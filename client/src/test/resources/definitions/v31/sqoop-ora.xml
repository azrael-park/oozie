<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<workflow-app xmlns="uri:oozie:workflow:0.3.1" name="test">
    <start to="sqoop-import"/>
    <action name="sqoop-import">
        <sqoop>
            <job-tracker>${jobTracker}</job-tracker>
            <name-node>${nameNode}</name-node>
            <prepare>
                <delete path="/user/qaerica/cdr_ora11g"/>
            </prepare>
            <arg>import</arg>
            <arg>--connect</arg>
            <arg>jdbc:oracle:thin:@14.63.226.231:1521:Ora11g</arg>
            <arg>--username</arg>
            <arg>NDAP</arg>
            <arg>--password</arg>
            <arg>ndap</arg>
            <arg>--table</arg>
            <arg>NDAP.CDR_ORA11G</arg>
            <arg>--target-dir</arg>
            <arg>/user/qaerica/cdr_ora11g</arg>
            <arg>--hive-import</arg>
            <arg>--hive-table</arg>
            <arg>cdr_ora11g</arg>
            <arg>--verbose</arg>
            <arg>-m</arg>
            <arg>1</arg>
        </sqoop>
        <ok to="end"/>
        <error to="fail"/>
    </action>
    <kill name="fail">
        <message>failed, error message[${wf:errorMessage(wf:lastErrorNode())}]</message>
    </kill>
    <end name="end"/>
</workflow-app>
